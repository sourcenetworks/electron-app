{
  "name": "source-client",
  "version": "0.0.1",
  "description": "source client",
  "main": "main.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "lint": "eslint renderer test *.js",
    "hot-server": "node server.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron ./",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "webpack:prod": "webpack --config webpack.config.production.js"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "author": "Source Co.",
  "license": "UNLICENSED",
  "devDependencies": {
    "asar": "^0.10.0",
    "babel-core": "^6.2.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-airbnb": "^2.2.1",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.26.1",
    "del": "^2.2.0",
    "electron-debug": "^0.5.2",
    "electron-devtools-installer": "^2.1.0",
    "electron-packager": "^6.0.2",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jsdom": "^8.2.0",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^4.4.0",
    "prettier": "^0.21.0",
    "redux-logger": "^2.6.1",
    "remote-redux-devtools": "^0.1.5",
    "remotedev-extension": "0.0.9",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "electron": "^1.4.15",
    "@sourcenetworks/background-lib": "0.0.2",
    "electron-promise-ipc": "^0.0.4",
    "lodash": "^4.17.4",
    "menubar": "^5.2.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-fontawesome": "^1.5.0",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "redux-pack": "^0.1.4",
    "redux-thunk": "^2.0.1",
    "styled-components": "^1.4.4"
  }
}
